generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model settings {
    id      String      @db.VarChar(255) @id
    data    String      @default("{}")

    @@map("settings")
}

model warns {
    guildId String
    userId  String
    caseId  String      @id
    moderator String    @db.VarChar(22)
    reason  String
    timestamp   String

    @@map("warns")
}
